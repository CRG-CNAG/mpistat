syntax = "proto3";

message MpistatMsg {
  bytes full_path = 1;
  bytes directory = 2;
  bytes file_name = 3;
  bytes suffix = 4;
  bytes suffix_class = 5;
  uint32 mode = 6;
  uint64 lsize = 7;
  uint64 size = 8;
  uint32 gid = 9;
  uint32 uid = 10;
  uint32 atime = 11;
  uint32 mtime = 12;
  uint64 depth = 13;
  uint64 blocks = 14;
  uint32 nlinks = 15;
  uint64 inode = 16;
  uint64 device= 17;
}
